# Firebase Configuration - Development
FIREBASE_API_KEY=your_dev_api_key_here
FIREBASE_AUTH_DOMAIN=protour-dev.firebaseapp.com
FIREBASE_PROJECT_ID=protour-dev
FIREBASE_STORAGE_BUCKET=protour-dev.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_dev_sender_id
FIREBASE_APP_ID=your_dev_app_id

# Firebase Configuration - Staging
FIREBASE_API_KEY_STAGING=your_staging_api_key_here
FIREBASE_AUTH_DOMAIN_STAGING=protour-staging.firebaseapp.com
FIREBASE_PROJECT_ID_STAGING=protour-staging
FIREBASE_STORAGE_BUCKET_STAGING=protour-staging.appspot.com
FIREBASE_MESSAGING_SENDER_ID_STAGING=your_staging_sender_id
FIREBASE_APP_ID_STAGING=your_staging_app_id

# Firebase Configuration - Production
FIREBASE_API_KEY_PROD=your_prod_api_key_here
FIREBASE_AUTH_DOMAIN_PROD=protour-prod.firebaseapp.com
FIREBASE_PROJECT_ID_PROD=protour-prod
FIREBASE_STORAGE_BUCKET_PROD=protour-prod.appspot.com
FIREBASE_MESSAGING_SENDER_ID_PROD=your_prod_sender_id
FIREBASE_APP_ID_PROD=your_prod_app_id

# Environment
NODE_ENV=development

# API Configuration
API_BASE_URL=http://localhost:5001/protour-dev/us-central1

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_DEBUG_LOGS=true
ENABLE_CRASHLYTICS=false

# External Services
SMS_API_KEY=your_sms_api_key_here
SMS_SENDER_ID=your_sms_sender_id_here